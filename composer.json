{
    "name": "zvps/royal-mail-shipping-rest-api-client",
    "description": "Royal Mail Shipping REST API Client",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/zVPS/royal-mail-shipping-rest-api-client",
    "license": "proprietary",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        },
        {
            "name": "zVPS.uk",
            "homepage": "https://zvps.uk"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "RoyalMail\\Shipping\\Rest\\Api\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "RoyalMail\\Shipping\\Rest\\Api\\Client\\" : "test/" }
    }
}
